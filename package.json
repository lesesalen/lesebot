{
  "name": "lesebot",
  "version": "0.6.0",
  "description": "A (helpful) Discord bot",
  "main": "dist/index.js",
  "repository": "https://github.com/sondr3/lesebot",
  "author": "Sondre Nilsen",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc",
    "clean": "rm -rf dist/ .cache/ .coverage/",
    "dev": "tsc --watch --incremental",
    "dev:run": "nodemon --watch src --ext ts --delay 2 dist/index.js",
    "lint": "eslint .",
    "style": "prettier --check ./src/**/*.ts",
    "release": "standard-version"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "axios": "^0.21.1",
    "cheerio": "1.0.0-rc.5",
    "dateformat": "4.4.1",
    "dayjs": "1.10.3",
    "discord.js": "^12.5.1",
    "discord.js-commando": "github:discordjs/Commando#198d7604e3725ee88dceab9b5f296edb1b7580a5",
    "dotenv": "^8.2.0",
    "globby": "11.0.2",
    "lodash": "^4.17.20",
    "winston": "^3.3.3"
  },
  "optionalDependencies": {
    "sodium": "^3.0.2"
  },
  "prettier": "@sondr3/prettier",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@sondr3/eslint-config": "0.3.3",
    "@sondr3/prettier": "0.5.0",
    "@sondr3/tsconfig": "0.7.0",
    "@types/cheerio": "^0.22.23",
    "@types/dateformat": "^3.0.1",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.20",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "*",
    "eslint-plugin-functional": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-simple-import-sort": "*",
    "eslint-plugin-tsdoc": "*",
    "eslint-plugin-unicorn": "*",
    "husky": "4.3.7",
    "lint-staged": "^10.5.3",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1",
    "prettier-eslint": "*",
    "standard-version": "9.1.0",
    "typescript": "^4.1.3"
  }
}

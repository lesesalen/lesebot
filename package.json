{
  "name": "lesebot",
  "version": "0.4.4",
  "description": "A (helpful) Discord bot",
  "main": "dist/index.js",
  "repository": "https://github.com/sondr3/lesebot",
  "author": "Sondre Nilsen",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc",
    "clean": "rm -rf dist/ .cache/ .coverage/",
    "dev": "tsc --watch --incremental",
    "dev:run": "nodemon --watch src --ext ts --delay 2 dist/index.js",
    "lint": "eslint .",
    "style": "prettier --check ./src/**/*.ts",
    "release": "standard-version"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "dateformat": "^3.0.3",
    "dayjs": "^1.8.35",
    "discord.js": "^12.3.1",
    "discord.js-commando": "github:discordjs/Commando#c5f4b209e89c648cb67007684468b9c51f4aa502",
    "dotenv": "^8.2.0",
    "globby": "^11.0.1",
    "lodash": "^4.17.20",
    "winston": "^3.3.3"
  },
  "optionalDependencies": {
    "sodium": "^3.0.2"
  },
  "prettier": "@sondr3/prettier",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@sondr3/eslint-config-typescript": "^0.8.3",
    "@sondr3/prettier": "^0.2.1",
    "@sondr3/tsconfig": "^0.4.1",
    "@types/cheerio": "^0.22.21",
    "@types/dateformat": "^3.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.10.0",
    "@types/ws": "^7.2.6",
    "eslint": "^7.8.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
